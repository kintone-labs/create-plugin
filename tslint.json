{
  "extends": ["tslint:recommended", "tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "quotemark": {
      "severity": "error",
      "options": ["avoid-escape", "single"]
    },
    "no-var-requires": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "arrow-parens": false
  }
}
